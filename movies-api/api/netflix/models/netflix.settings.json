{
  "connection": "default",
  "collectionName": "Netflixes",
  "info": {
    "name": "Netflix"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "categories": {
      "collection": "category",
      "via": "netflixes",
      "dominant": true
    },
    "casts": {
      "collection": "cast",
      "via": "netflixes",
      "dominant": true
    },
    "price": {
      "type": "decimal"
    }
  }
}
